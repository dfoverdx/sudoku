{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "es2015",
    // "lib": [
    //   "dom",
    //   "es2015"
    // ],
    "types": [
      "jest"
    ],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": false,
    "typeRoots": [
      "../node_modules/@types",
      "../typings",
      "./typings"
    ]
  },
  "compileOnSave": true,
  "exclude": [
    "node_modules",
    "../coverage",
    "__tests__/**"
  ],
  "include": [
    "./**/*.ts",
    "../src/**/*.ts",
    "../typings/**/*.d.ts"
  ]
}